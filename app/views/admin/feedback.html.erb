<div class="row heading">
  <div class="large-12 columns">
    <%= image_tag "pin.png" %><h2>List of Feedback</h2>
  </div>
</div>

<div class="beerance_list">
  <div class="row hide-for-small purple_grad">
    <div class="large-4 columns">
      Category
    </div>
    <div class="large-3 columns">
      Email
    </div>
    <div class="large-5 columns">
      Comments
    </div>
  </div>
  <% @feedbacks.each.each do |f| %>
    <div class="row">
      <div class="large-4 columns">
        <%= f.category %>
      </div>
      <div class="large-3 columns color">
        <%= link_to f.user.email, admin_user_path(f.user) %>
      </div>
      <div class="large-5 columns">
        <%= f.comment %>
      </div>
    </div>
  <% end %>

</div>
