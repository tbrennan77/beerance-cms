<h3>List of Feedback</h3>
<table style="width: 100%;">
  <thead>
    <tr>
      <th>Category</th>
      <th>Email</th>
      <th>Details</th>
  </tr>
  </thead>
  <tbody>
  <% @feedbacks.each do |f| %>
    <tr>
      <td><%= f.category %></td>
      <td><%= f.user.email %></td>
      <td><%= f.comment %></td>
    </tr>
  <% end %>
  </tbody>
</table>
